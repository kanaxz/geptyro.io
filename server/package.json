{
 "name": "geptyro.io-server",
 "version": "1.19.0",
 "main": "index.js",
 "private": true,
 "scripts": {
  "start": "echo $ENV && docker-compose -f docker-compose.$ENV.yml -p geptyro-io-$ENV --profile $ENV --env-file ../envs/$ENV.env up --build -d",
  "stop": "docker-compose -f docker-compose.$ENV.yml -p geptyro-io-$ENV --profile $ENV --env-file ../envs/$ENV.env down",
  "prod:build-client": "npm --workspace=geptyro.io-client run build",
  "prod:start": "export ENV=prod && npm run prod:build-client && npm run start",
  "prod:stop": "export ENV=prod && npm run stop",
  "prod:restart": "npm run prod:stop && npm run prod:start",
  "dev:start": "export ENV=dev && npm run start && npm run dev:server",
  "dev:server": "export ENV=dev && node index",
  "dev:restart": "npm run dev:stop && npm run dev:start",
  "dev:stop": "export ENV=dev && npm run stop",
  "push": "sools-push geptyro.io-server"
 },
 "author": "",
 "license": "ISC",
 "description": "",
 "dependencies": {
  "dotenv": "^16.4.5",
  "express": "^4.19.2",
  "sools-core": "1.6.0",
  "sools-core-server": "1.3.0",
  "sools-express": "1.7.0",
  "sools-management-server": "1.3.0",
  "sools-migrations": "1.1.0",
  "sools-modeling": "1.3.0",
  "sools-modeling-server": "1.2.0",
  "sools-modeling-server-express": "1.1.0",
  "sools-mongo": "1.2.0"
 },
 "devDependencies": {
  "sools-dev": "^1.2.0"
 }
}