{
  "name": "geptyro.io",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "docker-compose -p geptyro-io-$ENV --profile $ENV --env-file ./envs/$ENV.env up --build -d",
    "stop": "docker-compose -p geptyro-io-$ENV --profile $ENV --env-file ./envs/$ENV.env down",
    "prod": "export ENV=prod && npm run start",
    "stop-prod": "export ENV=prod && npm run stop",
    "dev": "export ENV=dev && npm run start",
    "stop-dev": "export ENV=dev && npm run stop"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "express": "^4.19.2"
  }
}